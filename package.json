{
	"name": "imperial.js",
	"version": "3.0.0-6",
	"description": "Imperial API wrapper for Node.js",
	"main": "./lib/index.js",
	"types": "./lib/index.d.ts",
	"scripts": {
		"build": "rimraf lib && tsc",
		"lint": "eslint src",
		"format": "prettier --write \"src/**/*.ts\"",
		"test": "jest",
		"ci": "rm -rf node_modules && yarn install --frozen-lockfile",
		"postinstall": "husky install",
		"prepublishOnly": "pinst --disable",
		"postpublish": "pinst --enable"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/imperialbin/imperial.js.git"
	},
	"keywords": [
		"imperial",
		"imperialb.in"
	],
	"author": "pxseu <contact.pxseu@gmail.com>",
	"license": "MPL-2.0",
	"bugs": {
		"url": "https://github.com/imperialbin/imperial.js/issues"
	},
	"homepage": "https://github.com/imperialbin/imperial.js#readme",
	"dependencies": {
		"abort-controller": "^3.0.0",
		"node-fetch": "^2.6.1"
	},
	"engines": {
		"node": ">=8"
	},
	"devDependencies": {
		"@types/jest": "^26.0.20",
		"@types/node": "^16.9.1",
		"@types/node-fetch": "^2.5.12",
		"@typescript-eslint/eslint-plugin": "^4.13.0",
		"@typescript-eslint/parser": "^4.13.0",
		"eslint": "^7.17.0",
		"eslint-config-airbnb-typescript": "^12.3.1",
		"eslint-config-prettier": "^7.1.0",
		"eslint-plugin-import": "^2.22.1",
		"fetch-mock-jest": "^1.5.1",
		"husky": "^6.0.0",
		"jest": "^26.6.3",
		"pinst": "^2.1.6",
		"prettier": "^2.2.1",
		"rimraf": "^3.0.2",
		"ts-jest": "^26.5.1",
		"typescript": "^4.1.5"
	},
	"jest": {
		"roots": [
			"<rootDir>/src"
		],
		"testMatch": [
			"**/test/**/*.test.ts"
		],
		"transform": {
			"^.+\\.ts$": "ts-jest"
		}
	}
}
